<!DOCTYPE html>
<html>
<head>
  <title>Pmc</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="container">
	
	<% if user_signed_in? %>
Welcome <%= current_user.email %>, <%= link_to "profile", profiles_path %> | <%= link_to "Account Settings", edit_user_registration_path %> | <%= link_to "log out", destroy_user_session_path, method: :delete %>
<%else%>
<%= link_to "sign_in", new_user_session_path %>
<%= link_to "log out", new_user_registration_path %>
<% end %>
</div>

<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

<%= yield %>

</body>
</html>
